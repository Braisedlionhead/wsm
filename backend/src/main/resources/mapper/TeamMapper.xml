<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bld.katana.mapper.TeamMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bld.entity.Team">
        <id column="team_id" property="teamId" />
        <result column="produced_quantity" property="producedQuantity" />
        <result column="is_busy" property="isBusy" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        team_id, produced_quantity, is_busy
    </sql>

</mapper>
